#include<iostream>
using namespace std;
int main()
{
    int n,i,j,k;
    cout<<"Enter number for Array :- ";
    cin>>n;
    int* a=new int[n*n];
    int* b=new int[n*n];
    int* c=new int[n*n];
    cout<<"Enter element of First Array :- ";
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            cin>>*(a+i+j);
        }
    }
    cout<<"Enter element of Second Array :- ";
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            cin>>*(b+i+j);
        }
    }
    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
		{
			*(c+i+j)=0;
            for(k=0;k<n;k++)
                *(c+i+j)+=*(a+i+k)*(*(b+k+j));

		}
    delete []a;
    delete []b;
    cout<<"Matrix of Multiplication is :-\n";
    for(i=0;i<n;i++)
    {
    	for(j=0;j<n;j++)
            cout<<*(c+i+j)<<" ";
        cout<<endl;
	}
    return 0;
}
